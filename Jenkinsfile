pipeline {
  agent any
  stages {
    stage('build'){
      steps{
        echo "build steps"
        sleep 5
      }
    }
    stage('test'){
      steps{
        echo "test step"
        sleep 5
      }
    }
    stage('deploy'){
      steps{
        echo "deploy step"
        sleep 5
      }
    }
    stage('docker'){
      steps{
        echo "image step"
        sleep 5
      }
    }
  }
} 
      
